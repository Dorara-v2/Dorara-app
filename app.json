{
  "expo": {
    "name": "Dorara",
    "slug": "Dorara",
    "version": "1.0.0",
    "notification": {
      "icon": "./assets/notification-logo.png",
      "color": "#f3a49d"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      ["@react-native-google-signin/google-signin"],
      ["@react-native-firebase/app"],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": ["-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"]
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-logo.png",
          "color": "#f3a49d",
          "androidMode": "default",
          "androidCollapsedTitle": "Dorara",
          "androidImportance": "max",
          "androidShowBadge": true
        }
      ]
    ]
    ,
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3a49d"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calc.dorara"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f3a49d"
      },
      "package": "com.calc.dorara",
      "enableDangerousExperimentalLeanBuilds": true,
      "useNextNotificationsApi": true
    },
    "extra": {
      "eas": {
        "projectId": "25e1d6be-a2af-4913-995a-bdb5f627b076"
      }
    }
  }
}
